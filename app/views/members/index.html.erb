<h1>Team Members</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>SID</th>
      <th>Email</th>
      <th>About</th>
      
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
    <tr>
        <td><%= member.name %></td>
        <td><%= member.SID %></td>
        <td><%= member.email %></td>
        <td><%= member.bio %></td>
    </tr>
    <% end %>
  </tbody>
</table>